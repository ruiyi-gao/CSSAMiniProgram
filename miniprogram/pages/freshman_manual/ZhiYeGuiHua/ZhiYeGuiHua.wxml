<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="TeBieTuiJian" style="background-color: rgb(249, 229, 166); height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-appreciate" bindtap="TeBieTuiJian" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="TeBieTuiJian" style="font-size:40rpx; color:rgb(41,40,53)">{{name_on_list}}</text>
    </view>
</view>

<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="XiaoWaiShiXi" style="background-image: url('https://gtcssaminiprogram.oss-cn-beijing.aliyuncs.com/miniprogrampic/XiaowaiUpdate.jpg');height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-form" bindtap="XiaoWaiShiXi" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="XiaoWaiShiXi" style="font-size:40rpx; color:rgb(41,40,53)">校外实习</text>
    </view>
</view>

<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="XiaoNeiShiXi" style="background-image: url('https://gtcssaminiprogram.oss-cn-beijing.aliyuncs.com/miniprogrampic/XiaoneiShixi.png');height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-github" bindtap="XiaoNeiShiXi" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="XiaoNeiShiXi" style="font-size:40rpx; color:rgb(41,40,53)">校内实习</text>
    </view>
</view>



<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="SSN_CPT" style="background-image: url('https://gtcssaminiprogram.oss-cn-beijing.aliyuncs.com/miniprogrampic/SSN3.jpeg');height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-people" bindtap="SSN_CPT" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="SSN_CPT" style="font-size:40rpx; color:rgb(41,40,53)"decode="{{true}}" >SSN&ensp;&&ensp;CPT</text>
    </view>
</view>
