<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="TeBieTuiJian"  style="background-color: rgb(249, 229, 166); height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-appreciate" bindtap="TeBieTuiJian" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="TeBieTuiJian" style="font-size:40rpx; color:rgb(41,40,53)">{{name_on_list}}</text>
    </view>
</view>

<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="WellsFargo" style="background-color: white;height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-card" bindtap="WellsFargo" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="WellsFargo" style="font-size:40rpx; color:rgb(41,40,53);opacity:1.5">Wells Fargo</text>
    </view>
</view>



<view class="bg-img radius shadow-blur margin-top bg-img" bindtap="BOA" style="background-color: white;height:{{height}}px; width:{{width}}px;border-radius: 20px;left:{{leftBorderWidth}}px">
    <view style="display: flex;justify-content: center; height: 100%; align-items: center; flex-direction: column;">
        <view class="cuIcon-card" bindtap="BOA" style="font-size: 100rpx; color:rgb(41,40,53)"></view>
        <text bindtap="BOA" style="font-size:40rpx; color:rgb(41,40,53)">Bank of America</text>
    </view>
</view>